<?xml version='1.0' encoding='utf-8'?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>FILL THIS IN</h:title>
    <model>
      <instance>
        <faims id="FILL_THIS_IN">
          <!--START: Style-->
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <!--END: Style-->
          <Login>
            <Author>
              <Login/>
            </Author>
          </Login>
          <Admin>
            <Home>
              <Create_New_Record/>
              <Field_Entries/>
            </Home>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_List/>
            </Search>
            <Seed_ID>
              <Next_Bore_ID/>
            </Seed_ID>
          </Admin>
          <Field_Entry>
            <Physiochemical>
              <Bore_ID/>
              <Field_Entry_timestamp/>
              <Catchment/>
              <Monitoring_Equipment_Present/>
              <Time_Removed/>
              <Time_Replaced/>
              <Min_Slot/>
              <Max_Slot/>
              <Depth_to_Water/>
              <Casing_Height/>
              <Colgroup_1>
                <Col_1>
                  <Temp_30L/>
                  <pH_30L/>
                  <EC_uS_cm_30/>
                  <DO_mg_L_30/>
                  <ORP_30L/>
                </Col_1>
                <Col_2>
                  <Temp_180L/>
                  <pH_180L/>
                  <EC_uS_cm_180/>
                  <DO_mg_L_180/>
                  <ORP_180L/>
                </Col_2>
              </Colgroup_1>
            </Physiochemical>
            <Stygofaunal>
              <Number_of_Jars_30L/>
              <Sed_Volume_Remaining_30/>
              <Number_of_Jars_180L/>
              <Sed_Volume_Remaining_180/>
              <Ethanol_Used/>
              <Notes/>
            </Stygofaunal>
            <Water_Quality>
              <Nutrients_50ml_Collected/>
              <Metals_Filtered/>
              <Water_Qual_Collected/>
              <Pesticide_Dipstick/>
              <Microbial_Sample/>
              <Storage/>
              <Tube_Cleaned/>
            </Water_Quality>
            <Site_Characteristics>
              <Trees_5m_Tall_in_250m_Radius/>
              <Dominant_Vegetation/>
              <Sediment_Type/>
              <Sediment_Volume/>
              <Land_Use_5_km/>
              <Irrigated/>
              <Type_Evidence_of_Irrigation/>
              <Recharge_Volume_Pumped_Before_Empty/>
              <Recharge_Rate_m_sec/>
              <Notes/>
            </Site_Characteristics>
            <Microbial_Filtering_EOD>
              <M_Collection_Date/>
              <M_Collection_Time/>
              <Number_of_Membranes_Filtered/>
              <M1_Volume_Filtered_L/>
              <M1_Sediment_Added/>
              <M2_Volume_Filtered_L/>
              <M2_Sediment_Added/>
              <Time_Frozen/>
            </Microbial_Filtering_EOD>
          </Field_Entry>
        </faims>
      </instance>
      <bind nodeset="/faims/Admin/Seed_ID/Next_Bore_ID" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Time_Removed" type="time"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Time_Replaced" type="time"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Min_Slot" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Max_Slot" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Depth_to_Water" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Casing_Height" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_1/Temp_30L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_1/pH_30L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_1/EC_uS_cm_30" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_1/DO_mg_L_30" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_1/ORP_30L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_2/Temp_180L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_2/pH_180L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_2/EC_uS_cm_180" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_2/DO_mg_L_180" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Physiochemical/Colgroup_1/Col_2/ORP_180L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Stygofaunal/Number_of_Jars_30L" type="integer"/>
      <bind nodeset="/faims/Field_Entry/Stygofaunal/Sed_Volume_Remaining_30" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Stygofaunal/Number_of_Jars_180L" type="integer"/>
      <bind nodeset="/faims/Field_Entry/Stygofaunal/Sed_Volume_Remaining_180" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Stygofaunal/Ethanol_Used" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Site_Characteristics/Trees_5m_Tall_in_250m_Radius" type="integer"/>
      <bind nodeset="/faims/Field_Entry/Site_Characteristics/Recharge_Rate_m_sec" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Microbial_Filtering_EOD/M_Collection_Date" type="date"/>
      <bind nodeset="/faims/Field_Entry/Microbial_Filtering_EOD/M_Collection_Time" type="time"/>
      <bind nodeset="/faims/Field_Entry/Microbial_Filtering_EOD/Number_of_Membranes_Filtered" type="integer"/>
      <bind nodeset="/faims/Field_Entry/Microbial_Filtering_EOD/M1_Volume_Filtered_L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Microbial_Filtering_EOD/M2_Volume_Filtered_L" type="decimal"/>
      <bind nodeset="/faims/Field_Entry/Microbial_Filtering_EOD/Time_Frozen" type="time"/>
    </model>
  </h:head>
  <h:body>
    <!--START: Style-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <!--END: Style-->
    <group ref="Login">
      <label>{Login}</label>
      <group ref="Author" faims_scrollable="false">
        <label>{Author}</label>
        <select1 ref="Login">
          <label>{Login}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Admin">
      <label>{Admin}</label>
      <group ref="Home" faims_scrollable="false">
        <label>{Home}</label>
        <trigger ref="Create_New_Record">
          <label>{Create_New_Record}</label>
        </trigger>
        <select1 appearance="compact" ref="Field_Entries">
          <label>{Field_Entries}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Search}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="large">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search_Button}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Seed_ID">
        <label>{Seed_ID}</label>
        <input ref="Next_Bore_ID" faims_style_class="required">
          <label>{Next_Bore_ID}</label>
        </input>
      </group>
    </group>
    <group ref="Field_Entry" faims_archent_type="Field Entry">
      <label>{Field_Entry}</label>
      <group ref="Physiochemical">
        <label>{Physiochemical}</label>
        <input faims_attribute_name="Bore ID" faims_attribute_type="measure" ref="Bore_ID">
          <label>{Bore_ID}</label>
        </input>
        <input faims_attribute_name="Field Entry timestamp" faims_attribute_type="measure" ref="Field_Entry_timestamp" faims_read_only="true">
          <label>{Field_Entry_timestamp}</label>
        </input>
        <input faims_attribute_name="Catchment" faims_attribute_type="measure" ref="Catchment">
          <label>{Catchment}</label>
        </input>
        <select1 appearance="full" faims_attribute_name="Monitoring Equipment Present" faims_attribute_type="vocab" ref="Monitoring_Equipment_Present">
          <label>{Monitoring_Equipment_Present}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Time Removed" faims_attribute_type="measure" ref="Time_Removed">
          <label>{Time_Removed}</label>
        </input>
        <input faims_attribute_name="Time Replaced" faims_attribute_type="measure" ref="Time_Replaced">
          <label>{Time_Replaced}</label>
        </input>
        <input faims_attribute_name="Min Slot" faims_attribute_type="measure" ref="Min_Slot">
          <label>{Min_Slot}</label>
        </input>
        <input faims_attribute_name="Max Slot" faims_attribute_type="measure" ref="Max_Slot">
          <label>{Max_Slot}</label>
        </input>
        <input faims_attribute_name="Depth to Water" faims_attribute_type="measure" ref="Depth_to_Water">
          <label>{Depth_to_Water}</label>
        </input>
        <input faims_attribute_name="Casing Height" faims_attribute_type="measure" ref="Casing_Height">
          <label>{Casing_Height}</label>
        </input>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Temp 30L" faims_attribute_type="measure" ref="Temp_30L">
              <label>{Temp_30L}</label>
            </input>
            <input faims_attribute_name="pH 30L" faims_attribute_type="measure" ref="pH_30L">
              <label>{pH_30L}</label>
            </input>
            <input faims_attribute_name="EC uS cm 30" faims_attribute_type="measure" ref="EC_uS_cm_30">
              <label>{EC_uS_cm_30}</label>
            </input>
            <input faims_attribute_name="DO mg L 30" faims_attribute_type="measure" ref="DO_mg_L_30">
              <label>{DO_mg_L_30}</label>
            </input>
            <input faims_attribute_name="ORP 30L" faims_attribute_type="measure" ref="ORP_30L">
              <label>{ORP_30L}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Temp 180L" faims_attribute_type="measure" ref="Temp_180L">
              <label>{Temp_180L}</label>
            </input>
            <input faims_attribute_name="pH 180L" faims_attribute_type="measure" ref="pH_180L">
              <label>{pH_180L}</label>
            </input>
            <input faims_attribute_name="EC uS cm 180" faims_attribute_type="measure" ref="EC_uS_cm_180">
              <label>{EC_uS_cm_180}</label>
            </input>
            <input faims_attribute_name="DO mg L 180" faims_attribute_type="measure" ref="DO_mg_L_180">
              <label>{DO_mg_L_180}</label>
            </input>
            <input faims_attribute_name="ORP 180L" faims_attribute_type="measure" ref="ORP_180L">
              <label>{ORP_180L}</label>
            </input>
          </group>
        </group>
      </group>
      <group ref="Stygofaunal">
        <label>{Stygofaunal}</label>
        <input faims_attribute_name="Number of Jars 30L" faims_attribute_type="measure" ref="Number_of_Jars_30L">
          <label>{Number_of_Jars_30L}</label>
        </input>
        <input faims_attribute_name="Sed Volume Remaining 30" faims_attribute_type="measure" ref="Sed_Volume_Remaining_30">
          <label>{Sed_Volume_Remaining_30}</label>
        </input>
        <input faims_attribute_name="Number of Jars 180L" faims_attribute_type="measure" ref="Number_of_Jars_180L">
          <label>{Number_of_Jars_180L}</label>
        </input>
        <input faims_attribute_name="Sed Volume Remaining 180" faims_attribute_type="measure" ref="Sed_Volume_Remaining_180">
          <label>{Sed_Volume_Remaining_180}</label>
        </input>
        <input faims_attribute_name="Ethanol Used" faims_attribute_type="measure" ref="Ethanol_Used">
          <label>{Ethanol_Used}</label>
        </input>
        <input faims_attribute_name="Notes" faims_attribute_type="measure" ref="Notes">
          <label>{Notes}</label>
        </input>
      </group>
      <group ref="Water_Quality">
        <label>{Water_Quality}</label>
        <select1 appearance="full" faims_attribute_name="Nutrients 50ml Collected" faims_attribute_type="vocab" ref="Nutrients_50ml_Collected">
          <label>{Nutrients_50ml_Collected}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Metals Filtered" faims_attribute_type="vocab" ref="Metals_Filtered">
          <label>{Metals_Filtered}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Water Qual Collected" faims_attribute_type="vocab" ref="Water_Qual_Collected">
          <label>{Water_Qual_Collected}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Pesticide Dipstick" faims_attribute_type="vocab" ref="Pesticide_Dipstick">
          <label>{Pesticide_Dipstick}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Microbial Sample" faims_attribute_type="vocab" ref="Microbial_Sample">
          <label>{Microbial_Sample}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 faims_attribute_name="Storage" faims_attribute_type="vocab" ref="Storage">
          <label>{Storage}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Tube Cleaned" faims_attribute_type="vocab" ref="Tube_Cleaned">
          <label>{Tube_Cleaned}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Site_Characteristics">
        <label>{Site_Characteristics}</label>
        <input faims_attribute_name="Trees 5m Tall in 250m Radius" faims_attribute_type="measure" ref="Trees_5m_Tall_in_250m_Radius">
          <label>{Trees_5m_Tall_in_250m_Radius}</label>
        </input>
        <select faims_attribute_name="Dominant Vegetation" faims_attribute_type="vocab" ref="Dominant_Vegetation">
          <label>{Dominant_Vegetation}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select faims_attribute_name="Sediment Type" faims_attribute_type="vocab" ref="Sediment_Type">
          <label>{Sediment_Type}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select faims_attribute_name="Sediment Volume" faims_attribute_type="vocab" ref="Sediment_Volume">
          <label>{Sediment_Volume}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select faims_attribute_name="Land Use 5 km" faims_attribute_type="vocab" ref="Land_Use_5_km">
          <label>{Land_Use_5_km}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select1 appearance="full" faims_attribute_name="Irrigated" faims_attribute_type="vocab" ref="Irrigated">
          <label>{Irrigated}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Type Evidence of Irrigation" faims_attribute_type="measure" ref="Type_Evidence_of_Irrigation">
          <label>{Type_Evidence_of_Irrigation}</label>
        </input>
        <input faims_attribute_name="Recharge Volume Pumped Before Empty" faims_attribute_type="measure" ref="Recharge_Volume_Pumped_Before_Empty">
          <label>{Recharge_Volume_Pumped_Before_Empty}</label>
        </input>
        <input faims_attribute_name="Recharge Rate m sec" faims_attribute_type="measure" ref="Recharge_Rate_m_sec">
          <label>{Recharge_Rate_m_sec}</label>
        </input>
        <input faims_attribute_name="Notes" faims_attribute_type="measure" ref="Notes">
          <label>{Notes}</label>
        </input>
      </group>
      <group ref="Microbial_Filtering_EOD">
        <label>{Microbial_Filtering_EOD}</label>
        <input faims_attribute_name="M Collection Date" faims_attribute_type="measure" ref="M_Collection_Date">
          <label>{M_Collection_Date}</label>
        </input>
        <input faims_attribute_name="M Collection Time" faims_attribute_type="measure" ref="M_Collection_Time">
          <label>{M_Collection_Time}</label>
        </input>
        <input faims_attribute_name="Number of Membranes Filtered" faims_attribute_type="measure" ref="Number_of_Membranes_Filtered">
          <label>{Number_of_Membranes_Filtered}</label>
        </input>
        <input faims_attribute_name="M1 Volume Filtered L" faims_attribute_type="measure" ref="M1_Volume_Filtered_L">
          <label>{M1_Volume_Filtered_L}</label>
        </input>
        <select1 appearance="full" faims_attribute_name="M1 Sediment Added" faims_attribute_type="vocab" ref="M1_Sediment_Added">
          <label>{M1_Sediment_Added}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="M2 Volume Filtered L" faims_attribute_type="measure" ref="M2_Volume_Filtered_L">
          <label>{M2_Volume_Filtered_L}</label>
        </input>
        <select1 appearance="full" faims_attribute_name="M2 Sediment Added" faims_attribute_type="vocab" ref="M2_Sediment_Added">
          <label>{M2_Sediment_Added}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Time Frozen" faims_attribute_type="measure" ref="Time_Frozen">
          <label>{Time_Frozen}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
